    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/vue3-jest": "^29.1.1",
    "codecov": "^3.8.3",
    "core-js": "^3.8.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
