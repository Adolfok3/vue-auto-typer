    "vue-auto-writer",
    "vue-auto-typer",
    "writer",
    "auto-writer",
    "auto-typer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com:Adolfok3/vue-auto-writer.git"
  },
  "main": "./dist/VueAutoWriter.common.js",
  "files": [
    "dist/*"
