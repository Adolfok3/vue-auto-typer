  "homepage": "https://github.com/Adolfok3/vue-auto-writer#readme",
  "author": "Luiz Adolfo",
  "keywords": [
    "vue",
    "vuejs",
    "writer",
    "typer",
    "vue-components",
    "vue-writer",
